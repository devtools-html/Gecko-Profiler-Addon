{
  "title": "Gecko Profiler",
  "name": "geckoprofiler",
  "description": "Collect platform profiles from Firefox Desktop.",
  "author": "Markus Stange <mstange@themasta.com>",
  "license": "MIT",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^7.0.0",
    "husky": "^0.13.3",
    "jest": "^20.0.3",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.2.3",
    "regenerator-runtime": "^0.10.5",
    "web-ext": "^1.8.1",
    "webpack": "^2.5.1"
  },
  "repository": "https://github.com/devtools-html/Gecko-Profiler-Addon",
  "config": {
    "firefox": "nightly"
  },
  "scripts": {
    "start": "web-ext run --firefox=$npm_package_config_firefox",
    "dev": "webpack --progress --colors --watch",
    "build": "web-ext build -i README.md package.json *.rdf transition resources",
    "storybook": "start-storybook -s . -p 9001 -c .storybook",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"./!(node_modules)**/*.js\" \"./src/**/*.js\" *.js",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "./!(node_modules)**/*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-subscriber": "^1.1.0",
    "redux-thunk": "^2.2.0",
    "redux-webext": "^1.1.2"
  },
  "jest": {
    "setupFiles": [
      "jest-webextension-mock"
    ]
  }
}
